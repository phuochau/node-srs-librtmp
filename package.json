{
  "name": "node-srs-librtmp",
  "version": "1.0.0",
  "description": "Port srs-librtmp to node addon",
  "main": "index.js",
  "scripts": {
    "autogypi": "autogypi",
    "node-gyp": "node-gyp",
    "emcc-path": "emcc-path",
    "copyasm": "copyasm",
    "ndts": "ndts",
    "install": "node-gyp rebuild",
    "build:rtmp": "cd librtmp & sudo make SYS=darwin",
    "build:srs": "clang++ -dynamiclib -o srsrtmp.dylib ./lib/srs_librtmp.cpp ./lib/srs_injecter.cpp",
    "build:srs2": "g++ -c ./lib/srs_librtmp.cpp ./lib/srs_injecter.cpp"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/phuochau/node-srs-librtmp.git"
  },
  "keywords": [
    "srs",
    "librtmp",
    "node-addon"
  ],
  "author": "Hau Vo",
  "license": "MIT",
  "gypfile": true,
  "bugs": {
    "url": "https://github.com/phuochau/node-srs-librtmp/issues"
  },
  "homepage": "https://github.com/phuochau/node-srs-librtmp#readme",
  "dependencies": {
    "ffi": "^2.2.0",
    "ref": "^1.3.4",
    "sleep": "^5.1.1"
  }
}
